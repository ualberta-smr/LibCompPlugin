<idea-plugin>
  <version>1.0-SNAPSHOT</version>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="192.6603" until-build="192.*"/>
  <id>my_Plugin.LibraryCompare</id>
  <name>LibraryComparison</name>
  <vendor email="relhajj@ualberta.ca" url="http://cs.ualberta.ca">Rehab el hajj</vendor>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <action id="EditorBasics.EditorIllustrationAction" class="org.intellij.sdk.editor.EditorIllustrationAction" popup="true" text="Library Comparison" description="Replaces selected text with suggested library.">
      <add-to-group group-id="EditorPopupMenu" anchor="first"/>
    </action>
    <group id="MyPlugin.test" text="LibComp" popup="true">
      <action id="EditorBasics.EditorIllustrationAct" class="org.intellij.sdk.editor.UserProfileAction" popup="true" text="User Profile" description="Replaces selected text with suggested library.">
        <keyboard-shortcut keymap="$default" first-keystroke="control alt C" second-keystroke="G"/>
      </action>
      <action id="EditorBasics.EditorIllustrationAct3" class="org.intellij.sdk.editor.SendFeedbackAction" popup="true" text="Send Us Your Feedback" description="Replaces selected text with suggested library.">
        <keyboard-shortcut keymap="$default" first-keystroke="control alt G" second-keystroke="C"/>
      </action>
      <add-to-group group-id="ToolsMenu" anchor="first"/>
    </group>
  </actions>
  <application-components>
    <component>
      <implementation-class>MainClassPlugin</implementation-class>
    </component>
    <component>
      <implementation-class>ModuleClassComponent</implementation-class>
    </component>
  </application-components>
  <project-components>
    <component>
      <implementation-class>MainClassPlugin</implementation-class>
    </component>
  </project-components>
  <module-components>
    <component>
      <implementation-class>ModuleClassComponent</implementation-class>
    </component>
  </module-components>
</idea-plugin>
